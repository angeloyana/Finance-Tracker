{
  "name": "finance-tracker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:web": "npm run copy:sql-wasm && tsc -b && vite build",
    "build:native": "npm run remove:sql-wasm && tsc -b && vite build",
    "copy:sql-wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
    "remove:sql-wasm": "rimraf public/assets/sql-wasm.wasm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "tscheck": "tsc -b --noEmit",
    "tscheck:watch": "tsc -b --noEmit --watch",
    "format": "prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^6.0.2",
    "@capacitor/android": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-router": "^7.6.2",
    "sql.js": "^1.12.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "prettier": "^3.6.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
